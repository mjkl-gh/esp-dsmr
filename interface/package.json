        {
          "when": "multiline"
        }
      ],
      "react/jsx-first-prop-new-line": [
        1,
        "multiline"
      ],
      "@typescript-eslint/no-shadow": 1,
      "max-len": [
        1,
        {
          "code": 140
        }
      ],
      "arrow-parens": 1
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
